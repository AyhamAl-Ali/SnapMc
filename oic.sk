			if {_i} is 24:
				message "%{_space}%%{_color}%o ok"
			if {_i} is 25:
				message "%{_space}%%{_color}%o"
			if {_i} is 26:
				message "%{_space}%%{_color}%o ic"
			if {_i} is 27:
				message "%{_space}%%{_color}%o o ic"
			if {_i} is 28:
				message "%{_space}%%{_color}%o o"
			if {_i} is 29:
				message "%{_space}%%{_color}%o ok ^^^"
			if {_i} is 30:
				message "%{_space}%%{_color}%o ^^"
