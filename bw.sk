		delete {bw.numberOfTeamsAlive::%{_id}%}
		delete {bw.locations.diamondGenerator::%{_id}%::*}
		delete {bw.nobuild::%{_id}%::*}
		delete {bw.upgrades.sharpness::%{_id}%::*}
		delete {bw.locations.ironGenerator::%{_id}%::*}
		delete {bw.upgrades.protection::%{_id}%::*}
		
		
		stop
	
	if {event.name} is "bedwars":
		set {bedwars.eventgame} to {_id}
		delete {event.name}
	set {_x} to 1000 * {_id}
	set {_z} to 0
	set {_y} to 70
	set {_loc} to location({_x}, {_y}, {_z}, world "BedWars")
	add .5 to x coordinate of {_loc}
	add .5 to z coordinate of {_loc}	
	
	paste schematic "/home/snap/plugins/WorldEdit/schematics/bw_air.schematic" at {_loc} excluding air
	wait 2 second
	set {_i} to random integer from 4 to 7
	
	if {_i} is 1:
		paste schematic "plugins/WorldEdit/schematics/bw_test3" at {_loc} using editsession {_session2} without air
	else if {_i} is 2:
		paste schematic "plugins/WorldEdit/schematics/BowserCastle" at {_loc} using editsession {_session2} without air
	else if {_i} is 3:
		paste schematic "plugins/WorldEdit/schematics/randehhs_piece_of_shit" at {_loc} using editsession {_session2} without air
	else if {_i} is 4:
		paste schematic "/home/snap/plugins/WorldEdit/schematics/bw_dino.schematic" at {_loc} excluding air	
	else if {_i} is 5:
		paste schematic "/home/snap/plugins/WorldEdit/schematics/bw_god.schematic" at {_loc} excluding air	
	else if {_i} is 6:
		paste schematic "/home/snap/plugins/WorldEdit/schematics/bw_military.schematic" at {_loc} excluding air	
	else if {_i} is 7:
		paste schematic "/home/snap/plugins/WorldEdit/schematics/bw_cake.schematic" at {_loc} excluding air
	

	set {bw.locations.spawnWait::%{_id}%} to location of block 40 above {_loc}

	set {_cows} to 0
	loop 10 times:
		if {_cows} = 0:
			loop all cows:
				if loop-entity is in world "BedWars":
					add 1 to {_cows}
			if {_cows} = 0:
				wait 1 second
		else:
			stop loop
	wait 1 second
	loop all cows:
		if loop-entity is in world "BedWars":
			set {_loce} to location of loop-entity
			set {_name} to name of loop-entity
			set health of loop-entity to 0
			#Red Bed
			if "%{_name}%" = "Red Bed":
				set {bw.locations.bed::%{_id}%::red} to location of loop-entity
					
			#Blue Bed
			else if "%{_name}%" = "Blue Bed":
				set {bw.locations.bed::%{_id}%::blue} to location of loop-entity
					
			#Blue Bed
			else if "%{_name}%" = "Yellow Bed":
