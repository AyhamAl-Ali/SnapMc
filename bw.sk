#play ENTITY_EXPERIENCE_ORB_PICKUP to player at volume 1 and pitch 3
#https://hub.spigotmc.org/javadocs/spigot/org/bukkit/Sound.html


on load:
	delete {justfell::*}
	delete {bw_nojoin}

	
command /delbw:
	permission: is.op
	trigger:
		delete {bw.numberOfTeamsAlive::*}
		delete {bw.players.list::*}
		delete {bw.teams.size::*}

command /bwvars:
	permission: is.op
	trigger:
		message "Total Teams: %size of {bw.numberOfTeamsAlive::*}%"
		message "Player Lists size %size of {bw.players.list::*}%"
		message "Team sizes in memory %size of {bw.teams.size::*}%"
		loop 400 times:
			add 1 to {_i}
			if {bw.gameid::%{_i}%} is not set:
				set {_id} to {_i}
				stop loop
		message "Next Game ID: %{_id}%"

on spawn:
	if event-entity is in world "BedWars":
		if event.getSpawnReason().toString() is not "CUSTOM":
			cancel the event	
on load:
	delete {bw.gameid::*}	
	delete {bw_nojoin}
	
on hunger meter change:
	if player is in world "BedWars":
		set player's hunger to 20
		
		
