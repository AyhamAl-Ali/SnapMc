				loop blocks in radius 1 of block at {_loce}:
					set {bw.nobuild::%{_id}%::%location of loop-block%} to location of loop-block
					
			# Stores		
			else if "%{_name}%" = "Shop":	
				spawn Villager at {_loce}
				set name of last spawned entity to "&e&lShop"
				apply potion of slowness 50 to last spawned entity for 100 minutes

			# Diamond Generator
			else if "%{_name}%" = "Diamond Gen":
