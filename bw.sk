command /bw:
	aliases: bedwars, bedwar, sw, spongewars, spongewar, eggwar, eggwars
	trigger:
		if {bw_nojoin} is set:
			message "&8[&bSnap&8] &7Please wait while the current players join their game"

			stop
	
		if "%block at player%" contains "stair":
			message "&8[&bSnap&8] &cYou can't do that from a chair!"
			stop
		set {_i} to 0
		loop all players in world "BedWars":
			if "%regions at loop-player%" contains "bw_pickteam":
				add 1 to {_i}
		execute console command "/effect %player% clear"
		if {_i} > 0:
			teleport player to {pos::bw_pickteams}
			stop
		teleport player to {pos::bw_pickteams}
		startbw()

