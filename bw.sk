			# Red Iron Generator
			else if "%{_name}%" = "Green Iron Gen":
				set {bw.locations.ironGenerator::%{_id}%::green} to {_loce}
			# Red Iron Generator
			else if "%{_name}%" = "Blue Iron Gen":
				set {bw.locations.ironGenerator::%{_id}%::blue} to {_loce}
