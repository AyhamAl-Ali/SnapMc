on inventory click:
	if player does not have permission "is.op":
		if name of player's current inventory contains "cosmic shop":
			cancel event
		
command /shop [<text>] [<text>] [<int>] [<int>]:
	trigger:
		if player is in world "Wilderness":
			message "&8[&bSnap&8] &7Redirecting to &a/wildshop &7since &b/shop&7 is for normal survival"
			execute player command "/wildshop"
			stop
			
		if player does not have permission "is.cosmic":
			message "&8[&bSnap&8] &7Redirecting to &a/ah &7since &b/shop&7 is a Cosmic rank perk"
			execute player command "/ah"
			stop
			
		
		if arg-1 is not set:
			open chest with 6 row named "         -=[ &1Cosmic Shop &8]=-" to player
			wait 1 tick
			if name of current inventory of player != "         -=[ &1Cosmic Shop &8]=-":
				close inventory of player
				stop
			execute player command "shop blocks"
			stop
				
			set {_i} to 0
			loop 10 times:
				format slot {_i} of player with blue stained glass pane named "&b&l*" to run [make player execute command "/shop"]
				add 1 to {_i}				
			format slot 17 of player with blue stained glass pane named "&c&l*" to run [make player execute command "/shop"]
			format slot 36 of player with blue stained glass pane named "&c&l*" to run [make player execute command "/shop"]
			set {_i} to 44
			loop 10 times:
				format slot {_i} of player with blue stained glass pane named "&c&l*" to run [make player execute command "/shop"]
				add 1 to {_i}
				
